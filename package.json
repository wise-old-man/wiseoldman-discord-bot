{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "A Discord bot for the Wise Old Man projects (https://github.com/wise-old-man/wise-old-man/)",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "prestart": "sequelize db:migrate",
    "start": "pm2-runtime dist/index.js",
    "predev": "npm run build && sequelize db:migrate",
    "dev": "ts-node-dev --poll --respawn --transpile-only --ignore-watch node_modules src/index.ts",
    "migration:generate": "cross-env MIGRATING=false npx sequelize-cli migration:generate --name ",
    "setup:docker": "docker-compose up --build"
  },
  "keywords": [
    "discord",
    "bot",
    "osrs",
    "runescape",
    "oldschool",
    "wiseoldman",
    "wise-old-man"
  ],
  "author": "Psikoi",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/lodash": "^4.14.155",
    "@types/node": "^16.11.47",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "sequelize-cli": "^5.5.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@sapphire/discord.js-utilities": "^4.0.1",
    "@types/bluebird": "^3.5.32",
    "@types/validator": "^13.0.0",
    "axios": "^0.19.2",
    "canvas": "^2.6.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "discord.js": "^13.6.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.19",
    "moment": "^2.26.0",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.13",
    "sequelize-typescript": "^1.1.0"
  },
  "engines": {
    "node": ">=12.0"
  }
}
